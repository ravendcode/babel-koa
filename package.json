{
  "name": "babel-koa",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "ravendcode <ravendcode@gmail.com>",
  "engines": {
    "node": "^8.9.4"
  },
  "scripts": {
    "start": "export NODE_ENV=production || set NODE_ENV=production&& yarn build && node dist",
    "dev": "export DEBUG=app:* || set DEBUG=app:* && nodemon src --exec babel-node -e js,hbs,pug",
    "build": "yarn run clean && babel src -d dist -s --ignore spec.js,test.js",
    "clean": "rimraf ./dist",
    "test:unit": "nodemon --exec mocha --require babel-register src/**/*.spec.js",
    "test": "export NODE_ENV=test || set NODE_ENV=test&& yarn test:unit"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^5.0.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "jquery": "^3.3.1",
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "popper.js": "^1.12.9",
    "pug": "^2.0.0-rc.4",
    "rimraf": "^2.6.2"
  }
}
